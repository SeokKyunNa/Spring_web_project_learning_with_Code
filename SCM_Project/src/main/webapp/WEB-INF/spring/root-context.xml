<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Root Context: defines shared resources visible to all other web components -->

	<bean id="snsbean" class="model.sns.SnsBean" />
	<bean id="bizbean" class="model.biz.BizBean" />
	<bean id="bizfavoritebean" class="model.biz.BizFavoriteBean" />
	<bean id="applicantbean" class="model.applicant.ApplicantBean" />
	<bean id="qnaService" class="dao.qna.QnaDAOImpl"/>
	<bean id="bbsService" class="dao.msg.BbsDAOImpl" />
	<bean id="prfService" class="dao.prf.PrfDAOImpl" />
	<bean id="snsService" class="dao.sns.SnsDAO" />
	<bean id="memberService" class="dao.mem.MemberDAOImpl" />
	<bean id="bizService" class="dao.biz.BizDAO" />
	<bean id="applicantService" class="dao.applicant.ApplicantDAO" />

	<bean id="multipartResolver"
		class="org.springframework.web.multipart.commons.CommonsMultipartResolver" >
		<property name="maxUploadSize" value="30720000" />
	</bean>
</beans>
